<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.airbnb.lottie.LottieAnimationView
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:layout_constraintStart_toStartOf="@id/tv_empty"
        app:layout_constraintEnd_toEndOf="@id/tv_empty"
        app:layout_constraintBottom_toTopOf="@id/tv_empty"
        android:layout_marginBottom="8dp"
        app:lottie_fileName="errorview.json"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        />

    <TextView
        android:id="@+id/tv_empty"
        style="@style/TextView.patrick_hand_20_181823"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/oopss_something_went_wrong"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>